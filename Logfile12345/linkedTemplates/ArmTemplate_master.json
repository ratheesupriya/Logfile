{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Logfile12345"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"Linkedservice_SQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Linkedservice_SQL'"
		},
		"Sqlserver1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Sqlserver1'",
			"defaultValue": "Integrated Security=False;Data Source=@{linkedService().ServerName};Initial Catalog=@{linkedService().dbName};User ID=@{linkedService().UserName};Password=@{linkedService().Password}"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://supriyaop1.dfs.core.windows.net/"
		},
		"Integration_properties_pipeline2_parameters_sourcetable": {
			"type": "string",
			"defaultValue": "customers"
		},
		"Integration_properties_pipeline2_parameters_dbnamefrompipeline": {
			"type": "string",
			"defaultValue": "BD1"
		},
		"Integration_properties_pipeline2_parameters_destinationtable": {
			"type": "string",
			"defaultValue": "sales.orders"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Logfile12345_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"Sqlserver1_connectionString": {
						"value": "[parameters('Sqlserver1_connectionString')]"
					},
					"Linkedservice_SQL_connectionString": {
						"value": "[parameters('Linkedservice_SQL_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"Integration_properties_pipeline2_parameters_sourcetable": {
						"value": "[parameters('Integration_properties_pipeline2_parameters_sourcetable')]"
					},
					"Integration_properties_pipeline2_parameters_dbnamefrompipeline": {
						"value": "[parameters('Integration_properties_pipeline2_parameters_dbnamefrompipeline')]"
					},
					"Integration_properties_pipeline2_parameters_destinationtable": {
						"value": "[parameters('Integration_properties_pipeline2_parameters_destinationtable')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}