{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "dataset_SQLTable",
						"type": "DatasetReference",
						"parameters": {
							"tablename": {
								"value": "@pipeline().parameters.sourcetable",
								"type": "Expression"
							},
							"dbnamefromdataset": {
								"value": "@pipeline().parameters.dbnamefrompipeline",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "dataset_SQLTable",
						"type": "DatasetReference",
						"parameters": {
							"tablename": {
								"value": "@pipeline().parameters.destinationtable",
								"type": "Expression"
							},
							"dbnamefromdataset": {
								"value": "@pipeline().parameters.dbnamefrompipeline",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"sourcetable": {
				"type": "string"
			},
			"dbnamefrompipeline": {
				"type": "string"
			},
			"destinationtable": {
				"type": "string"
			}
		},
		"annotations": []
	}
}